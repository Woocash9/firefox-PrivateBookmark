{

    "manifest_version": 2,
    "name": "PrivateBookmark",
    "version": "1.8",
    "author":"Lukasz Kwiatkowski",
    "incognito": "spanning",
    "description": "Opens bookmark marked as private in a new private tab.",
    "icons": {"76": "icons/private-bookmark.png"},
    "background": {"scripts": ["background.js"],"persistent":true},
    "options_ui": {"page": "options/options.html"},
    "permissions": ["<all_urls>", "tabs", "webRequest", "webRequestBlocking", "bookmarks", "storage"]  
  
}
